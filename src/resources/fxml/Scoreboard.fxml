<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="622.0" prefWidth="775.0"
  styleClass="hbox"
  stylesheets="@../css/main.css" xmlns="http://javafx.com/javafx/11.0.1"
  fx:controller="controllers.ScoreboardController">
  <children>
    <ButtonBar AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="1.0" AnchorPane.rightAnchor="1.0" layoutX="2.0"
      layoutY="346.0"
      maxHeight="-Infinity" minHeight="-Infinity" prefHeight="40.0"
      prefWidth="773.0"
      styleClass="button-bar">
      <buttons>
        <Button cancelButton="true" mnemonicParsing="false" onAction="#cmdBackClick" text="Back"
          fx:id="cmdBack"/>
      </buttons>
      <padding>
        <Insets bottom="7.0" left="7.0" right="7.0" top="7.0"/>
      </padding>
    </ButtonBar>
    <TableView AnchorPane.bottomAnchor="40.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"
      fixedCellSize="45.0" prefHeight="582.0" prefWidth="775.0"
      fx:id="tblTable">
      <columnResizePolicy>
        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
      </columnResizePolicy>
      <columns>
        <TableColumn editable="false" maxWidth="272.0" minWidth="272.0" prefWidth="272.0"
          resizable="false"
          sortType="DESCENDING" sortable="false" text="Nickname" fx:id="colNickname"/>
        <TableColumn editable="false" maxWidth="150.0" minWidth="150.0" prefWidth="150.0"
          sortType="DESCENDING"
          sortable="false" text="Games Played" fx:id="colGames"/>
        <TableColumn editable="false" maxWidth="100.0" minWidth="100.0" prefWidth="100.0"
          resizable="false"
          sortable="false" text="Wins" fx:id="colWins"/>
        <TableColumn editable="false" maxWidth="100.0" minWidth="100.0" prefWidth="100.0"
          resizable="false"
          sortable="false" text="Losses" fx:id="colLosses"/>
        <TableColumn editable="false" maxWidth="150.0" minWidth="150.0" prefWidth="150.0"
          resizable="false"
          sortType="DESCENDING" sortable="false" text="Win Percentage" fx:id="colPercentage"/>
      </columns>
    </TableView>
  </children>
</AnchorPane>
