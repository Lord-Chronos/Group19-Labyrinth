<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<Pane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity"
  minHeight="-Infinity"
  minWidth="-Infinity"
  prefHeight="736.0" prefWidth="300.0" styleClass="hbox" stylesheets="@../css/main.css"
  xmlns="http://javafx.com/javafx/11.0.1" fx:controller="controllers.MainMenuController">
  <children>
    <Button layoutX="30.0" layoutY="241.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
      minWidth="-Infinity" mnemonicParsing="false" onMouseClicked="#cmdNewClick"
      prefHeight="53.0"
      prefWidth="240.0" text="Start New Game" textFill="WHITE" fx:id="cmdNew">
      <font>
        <Font name="Roboto Light" size="16.0"/>
      </font>
    </Button>
    <Button layoutX="30.0" layoutY="321.0" maxHeight="-Infinity" maxWidth="-Infinity"
      minHeight="-Infinity"
      minWidth="-Infinity" mnemonicParsing="false" onMouseClicked="#cmdLoadClick"
      prefHeight="53.0"
      prefWidth="240.0" text="Load Game" textFill="WHITE" fx:id="cmdLoad">
      <font>
        <Font name="Roboto Light" size="16.0"/>
      </font>
    </Button>
    <Button layoutX="30.0" layoutY="411.0" maxHeight="-Infinity" maxWidth="-Infinity"
      minHeight="-Infinity"
      minWidth="-Infinity" mnemonicParsing="false" onMouseClicked="#cmdScoreboardClick"
      prefHeight="53.0"
      prefWidth="240.0" text="Scoreboard" textFill="WHITE" fx:id="cmdScoreboard">
      <font>
        <Font name="Roboto Light" size="16.0"/>
      </font>
    </Button>
    <Button layoutX="30.0" layoutY="491.0" maxHeight="-Infinity" maxWidth="-Infinity"
      minHeight="-Infinity"
      minWidth="-Infinity" mnemonicParsing="false" onMouseClicked="#cmdCreateProfileClick"
      prefHeight="53.0" prefWidth="240.0" text="Create a Profile"
      textFill="WHITE"
      fx:id="cmdMakeProfile">
      <font>
        <Font name="Roboto Light" size="16.0"/>
      </font>
    </Button>
    <Button layoutX="30.0" layoutY="571.0" maxHeight="-Infinity" maxWidth="-Infinity"
      minHeight="-Infinity"
      minWidth="-Infinity" mnemonicParsing="false" onMouseClicked="#cmdDeleteProfileClick"
      prefHeight="53.0" prefWidth="240.0" text="Delete a Profile"
      textFill="WHITE"
      fx:id="cmdDeleteProfile">
      <font>
        <Font name="Roboto Light" size="16.0"/>
      </font>
    </Button>
    <Button cancelButton="true" layoutX="30.0" layoutY="661.0" maxHeight="-Infinity" maxWidth="-Infinity"
      minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onMouseClicked="#quitClicked"
      prefHeight="53.0" prefWidth="240.0" text="Quit" textFill="WHITE"
      fx:id="cmdQuit">
      <font>
        <Font name="Roboto Light" size="16.0"/>
      </font>
    </Button>
    <ImageView fitHeight="72.0" fitWidth="272.0" layoutX="14.0" layoutY="14.0" pickOnBounds="true"
      fx:id="logo">
      <image>
        <Image url="@../menu/title.png"/>
      </image>
    </ImageView>
    <Label alignment="CENTER" contentDisplay="CENTER" layoutX="87.0" layoutY="109.0"
      text="Message of the Day:"
      textAlignment="CENTER"/>
    <Label alignment="CENTER" contentDisplay="CENTER" layoutX="6.0" layoutY="140.0"
      prefHeight="80.0"
      prefWidth="287.0" styleClass="label-small" text="this is a test message" textAlignment="CENTER"
      wrapText="true" fx:id="msgDayMessage"/>
    <Separator layoutX="50.0" layoutY="393.0" prefWidth="200.0"/>
    <Separator layoutX="50.0" layoutY="641.0" prefWidth="200.0"/>
    <Separator layoutX="50.0" layoutY="229.0" prefWidth="200.0"/>
    <Separator style="-fx-background-color: white;" layoutX="50.0" layoutY="97.0"
      prefWidth="200.0"/>

  </children>
