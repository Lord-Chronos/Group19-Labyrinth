<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.*?>
<Pane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
      minWidth="-Infinity"
      prefHeight="691.0" prefWidth="300.0" styleClass="pane-main"
      stylesheets="@../../resources/Misc/main.css" xmlns="http://javafx.com/javafx/11.0.1"
      fx:controller="fxml.MainMenuController">
    <children>
        <Button fx:id="cmdStartGame" layoutX="30.0" layoutY="203.0" maxHeight="-Infinity" maxWidth="-Infinity"
                minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onMouseClicked="#newGameClicked"
                prefHeight="53.0" prefWidth="240.0" styleClass="rollover-button"
                stylesheets="@../../resources/Misc/main.css" text="Start New Game" textFill="WHITE">
            <font>
                <Font name="Roboto Light" size="16.0"/>
            </font>
        </Button>
        <Button fx:id="cmdLoadGame" layoutX="30.0" layoutY="283.0" maxHeight="-Infinity" maxWidth="-Infinity"
                minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onMouseClicked="#loadGameClicked"
                prefHeight="53.0" prefWidth="240.0" styleClass="rollover-button"
                stylesheets="@../../resources/Misc/main.css" text="Load Game" textFill="WHITE">
            <font>
                <Font name="Roboto Light" size="16.0"/>
            </font>
        </Button>
        <Button fx:id="cmdScoreboard" layoutX="30.0" layoutY="373.0" maxHeight="-Infinity" maxWidth="-Infinity"
                minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onMouseClicked="#scoreboardClicked"
                prefHeight="53.0" prefWidth="240.0" style="-fx-background-color: transparent;"
                styleClass="rollover-button" stylesheets="@../../resources/Misc/main.css" text="Scoreboard"
                textFill="WHITE">
            <font>
                <Font name="Roboto Light" size="16.0"/>
            </font>
        </Button>
        <Button fx:id="cmdMakeProfile" layoutX="30.0" layoutY="453.0" maxHeight="-Infinity" maxWidth="-Infinity"
                minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onMouseClicked="#profileClicked"
                prefHeight="53.0" prefWidth="240.0" style="-fx-background-color: transparent;"
                styleClass="rollover-button" stylesheets="@../../resources/Misc/main.css" text="Make a Profile"
                textFill="WHITE">
            <font>
                <Font name="Roboto Light" size="16.0"/>
            </font>
        </Button>
        <Button fx:id="cmdMore" layoutX="30.0" layoutY="543.0" maxHeight="-Infinity" maxWidth="-Infinity"
                minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onMouseClicked="#moreClicked"
                prefHeight="53.0" prefWidth="240.0" style="-fx-background-color: transparent;"
                styleClass="rollover-button" stylesheets="@../../resources/Misc/main.css" text="More" textFill="WHITE">
            <font>
                <Font name="Roboto Light" size="16.0"/>
            </font>
        </Button>
        <Separator layoutX="50.0" layoutY="355.0" prefWidth="200.0" style="-fx-background-color: white;"/>
        <Separator layoutX="50.0" layoutY="524.0" prefWidth="200.0" style="-fx-background-color: white;"/>
        <Separator layoutX="50.0" layoutY="191.0" prefWidth="200.0" style="-fx-background-color: white;"/>
        <Button fx:id="cmdQuit" cancelButton="true" layoutX="30.0" layoutY="623.0" maxHeight="-Infinity"
                maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
                onMouseClicked="#quitClicked" prefHeight="53.0" prefWidth="240.0"
                style="-fx-background-color: transparent;" styleClass="rollover-button"
                stylesheets="@../../resources/Misc/main.css" text="Quit" textFill="WHITE">
            <font>
                <Font name="Roboto Light" size="16.0"/>
            </font>
        </Button>
        <ImageView fx:id="logo" fitHeight="72.0" fitWidth="272.0" layoutX="14.0" layoutY="14.0" pickOnBounds="true">
            <image>
                <Image url="@../../resources/Main_Menu_Style/title.png"/>
            </image>
        </ImageView>
        <Separator layoutX="50.0" layoutY="97.0" prefWidth="200.0" style="-fx-background-color: white;"/>
        <Label alignment="CENTER" layoutX="78.0" layoutY="109.0" stylesheets="@../../resources/Misc/main.css"
               text="Message of the Day:" textAlignment="CENTER"/>
        <Label fx:id="msgDayMessage" alignment="CENTER" contentDisplay="CENTER" layoutX="30.0" layoutY="142.0"
               prefHeight="41.0" prefWidth="240.0" styleClass="label-small" stylesheets="@../../resources/Misc/main.css"
               text="this is a test message" textAlignment="CENTER"/>

    </children>
</Pane>
