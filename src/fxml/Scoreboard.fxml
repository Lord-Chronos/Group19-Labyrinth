<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="622.0" prefWidth="775.0" styleClass="pane-main"
            stylesheets="@../../resources/Misc/main.css" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="fxml.ScoreboardController">
    <children>
        <TableView fx:id="tblTable" fixedCellSize="2.0" prefHeight="582.0" prefWidth="775.0" styleClass="pane-main"
                   stylesheets="@../../resources/Misc/main.css" AnchorPane.bottomAnchor="40.0"
                   AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <columns>
                <TableColumn fx:id="colNickname" editable="false" maxWidth="274.0" minWidth="274.0" prefWidth="274.0"
                             resizable="false" sortType="DESCENDING" sortable="false" text="Nickname"/>
                <TableColumn fx:id="colGames" editable="false" maxWidth="150.0" minWidth="150.0" prefWidth="150.0"
                             sortType="DESCENDING" sortable="false" text="Games Played"/>
                <TableColumn fx:id="colWins" editable="false" maxWidth="100.0" minWidth="100.0" prefWidth="100.0"
                             resizable="false" sortable="false" text="Wins"/>
                <TableColumn fx:id="colLosses" editable="false" maxWidth="100.0" minWidth="100.0" prefWidth="100.0"
                             resizable="false" sortable="false" text="Losses"/>
                <TableColumn fx:id="colPercentage" editable="false" maxWidth="150.0" minWidth="150.0" prefWidth="150.0"
                             resizable="false" sortType="DESCENDING" sortable="false" text="Win Percentage"/>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>
        <ButtonBar layoutX="89.0" layoutY="346.0" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="40.0"
                   prefWidth="200.0" style="-fx-background-color: transparent;" styleClass="border"
                   stylesheets="@../../resources/Misc/main.css" AnchorPane.bottomAnchor="0.0"
                   AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
            <buttons>
                <Button fx:id="cmdBack" cancelButton="true" mnemonicParsing="false" onAction="#cmdBackClick"
                        styleClass="rollover-button-small" stylesheets="@../../resources/Misc/main.css" text="Back"/>
            </buttons>
            <padding>
                <Insets bottom="7.0" left="7.0" right="7.0" top="7.0"/>
            </padding>
        </ButtonBar>
    </children>
</AnchorPane>
